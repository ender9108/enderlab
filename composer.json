{
    "name": "enderlab/psr15-middle-earth-framework",
    "description": "Micro framework middleware PSR-15",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Berthelot",
            "email": "alexandreberthelot9108@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Api\\": "app/api/",
            "EnderLab\\": "src/"
        }
    },
    "require": {
        "psr/http-message": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "guzzlehttp/psr7": "^1.4",
        "http-interop/response-sender": "^1.0",
        "http-interop/http-middleware": "^0.4.1",
        "php-di/php-di": "^5.4",
        "zendframework/zend-expressive-fastroute": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2.0",
        "middlewares/whoops": "^0.4.1",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.5",
        "satooshi/php-coveralls": "^1.0",
        "monolog/monolog": "^1.23"
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public/ -d display_errors=1"
    }
}
